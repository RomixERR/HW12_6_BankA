<Window x:Class="HW12_6_BankA.StartWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HW12_6_BankA"
        mc:Ignorable="d"
        Title="StartWindow"
        Height="150" Width="500"
        MaxHeight="150" MaxWidth="500"
        MinHeight="150" MinWidth="500"
        >
    <StackPanel>
        <TextBlock x:Name="textBlock" TextWrapping="Wrap"
                   Text="Банк /А/"
                   TextAlignment="Center" FontSize="24" FontWeight="Bold" FontFamily="Times New Roman"/>
        <TextBlock x:Name="textBlock2" TextWrapping="Wrap"
                   Text="Выбор работника:"
                   TextAlignment="Center" FontSize="14" FontFamily="Times New Roman"/>
        <ComboBox x:Name="comboBox" Width="270" SelectionChanged="comboBox_SelectionChanged"/>

    </StackPanel>

</Window>
